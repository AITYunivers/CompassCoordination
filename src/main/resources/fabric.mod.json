{
  "schemaVersion": 1,
  "id": "compass_coordination",
  "version": "${version}",

  "name": "Compass Coordination",
  "description": "A simple mod that adds coordinates to the player's UI when holding a compass. Inspired by CompassCoords by Insane96",
  "authors": [
    "Yunivers"
  ],
  "contact": {
    "homepage": "https://github.com/AITYunivers/CompassCoordination",
    "sources": "https://github.com/AITYunivers/CompassCoordination",
    "issues": "https://github.com/AITYunivers/CompassCoordination/issues"
  },

  "license": "MIT",
  "icon": "assets/CompassCoordination/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "io.github.yunivers.CompassCoordination.events.regui.InitHudWidgets"
    ],
    "gcapi3": [
      "io.github.yunivers.CompassCoordination.config.Config"
    ]
  },

  "depends": {
    "minecraft": "1.0.0-beta.7.3",
    "stationapi": ">=2.0.0-alpha.5.2",
    "regui": ">=1.1.8"
  },
  "suggests": {
    "gcapi3": "*"
  }
}
